{
  "name": "chroma-react-es6-starter",
  "version": "0.0.1",
  "description": "Chroma React ES6 Starter",
  "scripts": {
    "postinstall": "./node_modules/.bin/selenium-standalone install",
    "dev": "node_modules/.bin/nodemon index",
    "build": "rm -rf dist && mkdir dist && cp ./src/index.html ./dist/ && NODE_ENV=production node_modules/.bin/webpack --config ./webpack.config.js --progress --profile --colors",
    "eslint": "node_modules/.bin/eslint ./**/*.js",
    "test": "NODE_ENV=test node_modules/.bin/mocha ./test/unit --require ./test/test-init --compilers js:babel-core/register --recursive",
    "test:watch": "npm test -- -w",
    "test:e2e": "node_modules/.bin/nightwatch --test",
    "test:coverage": "node_modules/isparta/bin/isparta cover --include-all-sources --report text --report html node_modules/mocha/bin/_mocha -- --require ./test/test-init --compilers js:babel-core/register --recursive",
    "docs": "node_modules/.bin/docco {src,test}/{**/*,*} -l linear"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:chromasports/chroma-react-es6-starter.git"
  },
  "homepage": "https://github.com/chromasports/chroma-react-es6-starter/blob/master/README.md",
  "private": true,
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "docco": "^0.7.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.7.2",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "nightwatch": "^0.8.15",
    "node-jsdom": "^3.1.5",
    "nodemon": "^1.8.1",
    "react-addons-test-utils": "^0.14.7",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "dexie": "^1.2.0",
    "history": "^1.17.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.1.2",
    "react-router": "^1.0.3",
    "redux": "^3.1.7",
    "redux-saga": "^0.6.0",
    "redux-thunk": "^1.0.3",
    "selenium-standalone": "^4.9.0"
  }
}
